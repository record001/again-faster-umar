<!doctype html>
<html>
<head>
  <title>{{ page_title }}</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="{{ page_description | escape }}">
  <link rel="canonical" href="{{ canonical_url }}">
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  {{ content_for_header }} <!-- Header hook for plugins --> 

  <style>
             {% assign header_font = settings.heading_font -%}
            {{ header_font | font_face }}

            {% assign body_text_font = settings.base_font_new -%}
            {{ body_text_font | font_face }}

    :root {        
               
            --primary-font-family: {{- header_font.family  -}};
            --secondary-font-family: {{- body_text_font.family  -}}; 
          }
  </style>

  <!-- fonts.css is not used, instate theme settings is used above-->
  {{ "style.fonts.css" | asset_url | stylesheet_tag}}

  {{ "style.base.css" | asset_url | stylesheet_tag }}
  {{ "style.card.css" | asset_url | stylesheet_tag }}
  {{ "style.yotpo.css" | asset_url | stylesheet_tag }}
 


  
</head>
<body  class="{% if template == "index" %}home{% endif %}" >
  
  {% render "card-drawer"%}

  <header>
     {% section 'announcement-bar' %}
   
    {% section 'header-nav' %}
  </header>
  <main role="main">
    {{ content_for_layout }}
  </main>

<footer>
  {% section "footer" %}
</footer>


  
